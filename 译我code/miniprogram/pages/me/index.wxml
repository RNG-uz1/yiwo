
  <view class="introduce" wx:if="{{userInfo != null}}">
    <image class="userHead" src="{{userInfo.avatarUrl}}"></image>
    <view class="nickName" >昵称：{{userInfo.nickName}}</view>

    
  </view>

  <button class="login_button" bindtap="login" wx:if="{{userInfo == null}}">点击登录</button>

  <view class="myTag">
    <view class="title">
      <image class="iconTag" src="https://7969-yiwo-4gsw4af5a186d6b7-1308472708.tcb.qcloud.la/cloudbase-cms/upload/2022-01-14/y8w2qhck41ipz5b7rryk6rv25ykmol3j_.png"></image>
      <view class="text">我的标签</view>
    </view>

  <view class="tags">
    <view class='btn_view'>
        <block wx:for="{{obtnArry}}"  >
          <view class='label_view' bindtap="isDel" data-value="{{item.name}}"   data-index="{{index}}">
            {{item.name}}
          </view>
        </block>
        <image bindtap="addinput"  class="img" src="https://7969-yiwo-4gsw4af5a186d6b7-1308472708.tcb.qcloud.la/cloudbase-cms/upload/2022-01-14/ef81jf48bmpmtqa5yd2msjyobor16kev_.png" ></image>
    </view>



  </view>
  
</view>

  <view class="watchMakesMe"  bindtap="gotoWatch">What Makes Me</view>

  <button open-type="feedback" class="feedBack">用户反馈</button>
